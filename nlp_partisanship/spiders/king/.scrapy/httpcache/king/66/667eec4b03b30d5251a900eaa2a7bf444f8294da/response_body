<script type="text/javascript">

   $(document).ready(function(){

	 	// check if the dic is on the page.

	    if($('#instamedia').length){


		    var feed = new Instafeed({
	    	get: 'user',
	    	userId:1469444469, // king
	    	accessToken: '1469444469.467ede5.62a53be7111c4eb982054220bdf487ea', // king
	        target: 'instamedia',
	    	sortBy: 'most-recent',
	        limit: '25',
	        resolution: 'standard_resolution',
	        template: '<div class="photo"><a href="{{link}}"><img src="{{image}}" /></a> <div class="time" data-ts="{{model.created_time}}"></div> <div class="caption">{{caption}}</div></div>',
	        success: function(obj){

		       $('#instahead .profilePic').html('<img src="'+obj.data[0].user.profile_picture+'" />');

	        },
	        after: function(){

		        var times = $('#instamedia .time');
		        for(i=0; i<times.length; i++){
			        var t = $(times[i]).data('ts');

			        var t_format = moment.unix(t).format('MM/DD/YYYY');
			        $(times[i]).html(t_format);

		        }

	        }
	    });

	    feed.run();

    }

   });

</script>




